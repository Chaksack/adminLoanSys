{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  // eslint-disable-next-line vue/multi-word-component-names\n  name: \"Login\",\n\n  data() {\n    return {\n      login: {\n        email: \"\",\n        password: \"\"\n      }\n    };\n  },\n\n  methods: {\n    handleSubmit() {\n      const extAPI = axios.create({\n        baseURL: \"http://nnuro-learn.herokuapp.com\",\n        timeout: 5000\n      }); // extAPI.interceptors.request.use((config) => {\n      //   config.headers[\"Authorization\"] =\n      //     \"Bearer \" + localStorage.getItem(\"token\");\n      // });\n\n      extAPI.post(\"/api-token/\", {\n        email: this.login.email,\n        password: this.login.password\n      }).then(response => {\n        console.log(response.data);\n        this.$router.push(\"/dashboard\");\n      }).catch(error => {\n        console.log(\"There was an error:\", error.data);\n      });\n    } //   return {\n    //   handleSubmit,\n    // }\n\n\n  }\n};","map":{"version":3,"mappings":"AAiHA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,eAAe;EACb;EACAC,IAAI,EAAE,OAFO;;EAGbC,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE;QAAEC,KAAK,EAAE,EAAT;QAAaC,QAAQ,EAAE;MAAvB;IADF,CAAP;EAGD,CAPY;;EAQbC,OAAO,EAAE;IACPC,YAAY,GAAG;MACb,MAAMC,MAAK,GAAIR,KAAK,CAACS,MAAN,CAAa;QAC1BC,OAAO,EAAE,kCADiB;QAE1BC,OAAO,EAAE;MAFiB,CAAb,CAAf,CADa,CAMb;MACA;MACA;MACA;;MAEAH,MAAK,CACFI,IADH,CACQ,aADR,EACuB;QACnBR,KAAK,EAAE,KAAKD,KAAL,CAAWC,KADC;QAEnBC,QAAQ,EAAE,KAAKF,KAAL,CAAWE;MAFF,CADvB,EAKGQ,IALH,CAKSC,QAAD,IAAc;QAClBC,OAAO,CAACC,GAAR,CAAYF,QAAQ,CAACZ,IAArB;QACA,KAAKe,OAAL,CAAaC,IAAb,CAAkB,YAAlB;MACD,CARH,EASGC,KATH,CASUC,KAAD,IAAW;QAChBL,OAAO,CAACC,GAAR,CAAY,qBAAZ,EAAmCI,KAAK,CAAClB,IAAzC;MACD,CAXH;IAYD,CAxBM,CAyBP;IACA;IACA;;;EA3BO;AARI,CAAf","names":["axios","name","data","login","email","password","methods","handleSubmit","extAPI","create","baseURL","timeout","post","then","response","console","log","$router","push","catch","error"],"sourceRoot":"","sources":["/Users/chakdahahsackey/pavelonv0.01/src/views/LogIn.vue"],"sourcesContent":["<template>\n  <!--\n    This example requires updating your template:\n\n    ```\n    <html class=\"h-full bg-gray-50\">\n    <body class=\"h-full\">\n    ```\n  -->\n  <div\n    class=\"max-w-md my-52 mx-auto w-auto bg-white rounded-lg border border-gray-200 items-center\"\n  >\n    <div class=\"min-h-full flex flex-col justify-center py-12 sm:px-6 lg:px-8\">\n      <div class=\"sm:mx-auto sm:w-full sm:max-w-md\">\n        <img\n          class=\"mx-auto h-12 w-auto\"\n          src=\"../assets/PavelonLogo.png\"\n          alt=\"Workflow\"\n        />\n        <p>\"Buy Now, Pay Later\"</p>\n        <h2 class=\"mt-6 text-center text-3xl font-extrabold text-gray-900\">\n          Sign in to your account\n        </h2>\n      </div>\n      <div class=\"mt-8 sm:mx-auto sm:w-full sm:max-w-md\">\n        <div>\n          <form\n            class=\"space-y-6\"\n            method=\"POST\"\n            v-on:submit.prevent=\"handleSubmit\"\n          >\n            <div>\n              <label\n                for=\"email\"\n                class=\"block text-sm font-medium text-gray-900\"\n              >\n              </label>\n              <div class=\"mt-1\">\n                <input\n                  placeholder=\"Email address\"\n                  id=\"email\"\n                  name=\"email\"\n                  type=\"email\"\n                  v-model=\"login.email\"\n                  autocomplete=\"email\"\n                  required=\"\"\n                  class=\"form-control appearance-none block w-full px-3 py-2 border border-gray-300 rounded-md shadow-sm placeholder-gray-400 focus:outline-none focus:ring-indigo-500 focus:border-indigo-500 sm:text-sm\"\n                />\n              </div>\n            </div>\n\n            <div>\n              <label\n                for=\"password\"\n                class=\"block text-sm font-medium text-gray-700\"\n              >\n              </label>\n              <div class=\"mt-1\">\n                <input\n                  placeholder=\"Password\"\n                  id=\"password\"\n                  name=\"password\"\n                  type=\"password\"\n                  v-model=\"login.password\"\n                  autocomplete=\"current-password\"\n                  required=\"\"\n                  class=\"appearance-none block w-full px-3 py-2 border border-gray-300 rounded-md shadow-sm placeholder-gray-400 focus:outline-none focus:ring-indigo-500 focus:border-indigo-500 sm:text-sm\"\n                />\n              </div>\n            </div>\n\n            <div class=\"flex items-center justify-between\">\n              <div class=\"flex items-center\">\n                <input\n                  id=\"remember-me\"\n                  name=\"remember-me\"\n                  type=\"checkbox\"\n                  class=\"h-4 w-4 text-indigo-600 focus:ring-indigo-500 border-gray-300 rounded\"\n                />\n                <label\n                  for=\"remember-me\"\n                  class=\"ml-2 block text-sm text-gray-900\"\n                >\n                  Remember me\n                </label>\n              </div>\n\n              <div class=\"text-sm\">\n                <a\n                  href=\"#\"\n                  class=\"font-medium text-indigo-600 hover:text-indigo-500\"\n                >\n                  Forgot your password?\n                </a>\n              </div>\n            </div>\n\n            <div>\n              <button\n                type=\"submit\"\n                class=\"w-full flex justify-center py-2 px-4 border border-transparent rounded-md shadow-sm text-sm font-medium text-white bg-gray-900 hover:bg-gray-700 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-gray-500\"\n              >\n                Log in\n              </button>\n            </div>\n          </form>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  // eslint-disable-next-line vue/multi-word-component-names\n  name: \"Login\",\n  data() {\n    return {\n      login: { email: \"\", password: \"\" },\n    };\n  },\n  methods: {\n    handleSubmit() {\n      const extAPI = axios.create({\n        baseURL: \"http://nnuro-learn.herokuapp.com\",\n        timeout: 5000,\n      });\n\n      // extAPI.interceptors.request.use((config) => {\n      //   config.headers[\"Authorization\"] =\n      //     \"Bearer \" + localStorage.getItem(\"token\");\n      // });\n\n      extAPI\n        .post(\"/api-token/\", {\n          email: this.login.email,\n          password: this.login.password,\n        })\n        .then((response) => {\n          console.log(response.data);\n          this.$router.push(\"/dashboard\");\n        })\n        .catch((error) => {\n          console.log(\"There was an error:\", error.data);\n        });\n    },\n    //   return {\n    //   handleSubmit,\n    // }\n  },\n};\n</script>\n\n<style></style>\n"]},"metadata":{},"sourceType":"module"}