{"ast":null,"code":"import { ref } from \"vue\";\nimport SideBar from \"../components/SideBar\";\nimport Footer from \"../components/Footer\";\nimport { Menu, MenuButton, MenuItem, MenuItems } from \"@headlessui/vue\";\nimport axios from \"axios\";\nimport { ArrowNarrowLeftIcon, ArrowNarrowRightIcon } from \"@heroicons/vue/solid\";\nimport { Dialog, DialogPanel, DialogTitle, TransitionChild, TransitionRoot } from \"@headlessui/vue\";\nimport { XIcon } from \"@heroicons/vue/outline\";\nimport { ChevronDownIcon } from \"@heroicons/vue/solid\";\nimport LocalStorageService from \"../APIs/token\";\nexport default {\n  // eslint-disable-next-line vue/multi-word-component-names\n  name: \"Dashboard\",\n  components: {\n    SideBar,\n    Footer,\n    ArrowNarrowLeftIcon,\n    ArrowNarrowRightIcon,\n    Dialog,\n    DialogPanel,\n    DialogTitle,\n    TransitionChild,\n    TransitionRoot,\n    XIcon,\n    Menu,\n    MenuButton,\n    MenuItem,\n    MenuItems,\n    ChevronDownIcon\n  },\n\n  data() {\n    return {\n      loans: []\n    };\n  },\n\n  methods: {\n    getCreditApp() {\n      const creditAPI = axios.create({\n        baseURL: \"http://sandbox.pavelon.com\",\n        headers: {\n          Authorization: \"Bearer \" + LocalStorageService.getAccessToken(),\n          \"x-api-key\": \"x64cRjHA3240SSdfdk\",\n          \"x-api-version\": \"v3\",\n          \"xbs-token\": LocalStorageService.getXbsToken()\n        }\n      }); // extAPI.interceptors.request.use((config) => {\n      //   config.headers[\"Authorization\"] =\n      //     \"Bearer \" + localStorage.getItem(\"token\");\n      // });\n\n      creditAPI.get(\"/api/credit\").then(response => {\n        this.getCreditApp = response.data.pageItems;\n        console.log(response.data);\n      }).catch(error => {\n        console.log(\"There was an error:\", error.data);\n      });\n    },\n\n    getCreditTrans() {\n      const transAPI = axios.create({\n        baseURL: \"http://sandbox.pavelon.com\",\n        headers: {\n          Authorization: \"Bearer \" + LocalStorageService.getAccessToken(),\n          \"x-api-key\": \"x64cRjHA3240SSdfdk\",\n          \"x-api-version\": \"v3\",\n          \"xbs-token\": LocalStorageService.getXbsToken()\n        }\n      });\n      transAPI.get(\"/api/credit/4/transactions\").then(response => {\n        this.getCreditTrans = response.data.pageItems;\n        console.log(response.data);\n      }).catch(error => {\n        console.log(\"There was an error:\", error.data);\n      });\n    } //   return {\n    //   handleSubmit,\n    // }\n\n\n  },\n\n  mounted() {\n    this.getCreditApp();\n  },\n\n  setup() {\n    let isOpen = ref(false);\n    let CreateBorrower = ref(null);\n    let isTable = ref(false);\n    let checkDetails = ref(null);\n    return {\n      isOpen,\n      CreateBorrower,\n      isTable,\n      checkDetails,\n\n      setIsOpen(value) {\n        isOpen.value = value;\n      },\n\n      setIsTable(value) {\n        isTable.value = value;\n      }\n\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA6nBA,SAASA,GAAT,QAAoB,KAApB;AACA,OAAOC,OAAP,MAAoB,uBAApB;AACA,OAAOC,MAAP,MAAmB,sBAAnB;AACA,SAASC,IAAT,EAAeC,UAAf,EAA2BC,QAA3B,EAAqCC,SAArC,QAAsD,iBAAtD;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,SACEC,mBADF,EAEEC,oBAFF,QAGO,sBAHP;AAIA,SACEC,MADF,EAEEC,WAFF,EAGEC,WAHF,EAIEC,eAJF,EAKEC,cALF,QAMO,iBANP;AAOA,SAASC,KAAT,QAAsB,wBAAtB;AACA,SAASC,eAAT,QAAgC,sBAAhC;AACA,OAAOC,mBAAP,MAAgC,eAAhC;AACA,eAAe;EACb;EACAC,IAAI,EAAE,WAFO;EAGbC,UAAU,EAAE;IACVlB,OADU;IAEVC,MAFU;IAGVM,mBAHU;IAIVC,oBAJU;IAKVC,MALU;IAMVC,WANU;IAOVC,WAPU;IAQVC,eARU;IASVC,cATU;IAUVC,KAVU;IAWVZ,IAXU;IAYVC,UAZU;IAaVC,QAbU;IAcVC,SAdU;IAeVU;EAfU,CAHC;;EAoBbI,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE;IADF,CAAP;EAGD,CAxBY;;EA0BbC,OAAO,EAAE;IACPC,YAAY,GAAG;MACb,MAAMC,SAAQ,GAAIjB,KAAK,CAACkB,MAAN,CAAa;QAC7BC,OAAO,EAAE,4BADoB;QAE7BC,OAAO,EAAE;UACPC,aAAa,EAAE,YAAYX,mBAAmB,CAACY,cAApB,EADpB;UAEP,aAAa,oBAFN;UAGP,iBAAiB,IAHV;UAIP,aAAaZ,mBAAmB,CAACa,WAApB;QAJN;MAFoB,CAAb,CAAlB,CADa,CAWb;MACA;MACA;MACA;;MAEAN,SAAQ,CACLO,GADH,CACO,aADP,EAEGC,IAFH,CAESC,QAAD,IAAc;QAClB,KAAKV,YAAL,GAAoBU,QAAQ,CAACb,IAAT,CAAcc,SAAlC;QACAC,OAAO,CAACC,GAAR,CAAYH,QAAQ,CAACb,IAArB;MACD,CALH,EAMGiB,KANH,CAMUC,KAAD,IAAW;QAChBH,OAAO,CAACC,GAAR,CAAY,qBAAZ,EAAmCE,KAAK,CAAClB,IAAzC;MACD,CARH;IASD,CA1BM;;IA2BPmB,cAAc,GAAG;MACf,MAAMC,QAAO,GAAIjC,KAAK,CAACkB,MAAN,CAAa;QAC5BC,OAAO,EAAE,4BADmB;QAE5BC,OAAO,EAAE;UACPC,aAAa,EAAE,YAAYX,mBAAmB,CAACY,cAApB,EADpB;UAEP,aAAa,oBAFN;UAGP,iBAAiB,IAHV;UAIP,aAAaZ,mBAAmB,CAACa,WAApB;QAJN;MAFmB,CAAb,CAAjB;MASAU,QAAO,CACJT,GADH,CACO,4BADP,EAEGC,IAFH,CAESC,QAAD,IAAc;QAClB,KAAKM,cAAL,GAAsBN,QAAQ,CAACb,IAAT,CAAcc,SAApC;QACAC,OAAO,CAACC,GAAR,CAAYH,QAAQ,CAACb,IAArB;MACD,CALH,EAMGiB,KANH,CAMUC,KAAD,IAAW;QAChBH,OAAO,CAACC,GAAR,CAAY,qBAAZ,EAAmCE,KAAK,CAAClB,IAAzC;MACD,CARH;IASD,CA9CM,CA+CP;IACA;IACA;;;EAjDO,CA1BI;;EA6EbqB,OAAO,GAAG;IACR,KAAKlB,YAAL;EACD,CA/EY;;EAgFbmB,KAAK,GAAG;IACN,IAAIC,MAAK,GAAI3C,GAAG,CAAC,KAAD,CAAhB;IACA,IAAI4C,cAAa,GAAI5C,GAAG,CAAC,IAAD,CAAxB;IACA,IAAI6C,OAAM,GAAI7C,GAAG,CAAC,KAAD,CAAjB;IACA,IAAI8C,YAAW,GAAI9C,GAAG,CAAC,IAAD,CAAtB;IAEA,OAAO;MACL2C,MADK;MAELC,cAFK;MAGLC,OAHK;MAILC,YAJK;;MAKLC,SAAS,CAACC,KAAD,EAAQ;QACfL,MAAM,CAACK,KAAP,GAAeA,KAAf;MACD,CAPI;;MAQLC,UAAU,CAACD,KAAD,EAAQ;QAChBH,OAAO,CAACG,KAAR,GAAgBA,KAAhB;MACD;;IAVI,CAAP;EAYD;;AAlGY,CAAf","names":["ref","SideBar","Footer","Menu","MenuButton","MenuItem","MenuItems","axios","ArrowNarrowLeftIcon","ArrowNarrowRightIcon","Dialog","DialogPanel","DialogTitle","TransitionChild","TransitionRoot","XIcon","ChevronDownIcon","LocalStorageService","name","components","data","loans","methods","getCreditApp","creditAPI","create","baseURL","headers","Authorization","getAccessToken","getXbsToken","get","then","response","pageItems","console","log","catch","error","getCreditTrans","transAPI","mounted","setup","isOpen","CreateBorrower","isTable","checkDetails","setIsOpen","value","setIsTable"],"sourceRoot":"","sources":["/Users/chakdahahsackey/pavelonv0.01/src/views/Dashboard.vue"],"sourcesContent":["/* eslint-disable no-undef */\n<template>\n  <div class=\"flex min-h-full\">\n    <div>\n      <SideBar />\n    </div>\n    <div class=\"relative top-16 right-11 px-10 sm:px-6 lg:px-8 flex-1\">\n      <div class=\"sm:flex sm:items-center\">\n        <div class=\"mt-4 sm:mt-0 sm:ml-11 sm:flex-none\">\n          <Menu>\n            <MenuButton\n              class=\"inline-flex justify-center w-full font-sans font-bold text-2xl rounded-md border-none border-white px-4 py-2 bg-white text-gray-700 hover:bg-gray-200 focus:outline-none\"\n            >\n              Applications\n              <ChevronDownIcon class=\"-mr-1 ml-2 h-5 w-5\" aria-hidden=\"true\" />\n            </MenuButton>\n\n            <transition\n              enter-active-class=\"transition ease-out duration-100\"\n              enter-from-class=\"transform opacity-0 scale-95\"\n              enter-to-class=\"transform opacity-100 scale-100\"\n              leave-active-class=\"transition ease-in duration-75\"\n              leave-from-class=\"transform opacity-100 scale-100\"\n              leave-to-class=\"transform opacity-0 scale-95\"\n            >\n              <MenuItems\n                class=\"origin-top-left absolute left-11 mt-2 w-56 rounded-md shadow-lg bg-white ring-1 ring-black ring-opacity-5 focus:outline-none\"\n              >\n                <div class=\"py-1\">\n                  <MenuItem v-slot=\"{ active }\">\n                    <a\n                      href=\"#\"\n                      :class=\"[\n                        active ? 'bg-gray-100 text-gray-900' : 'text-gray-700',\n                        'block px-4 py-2 text-sm',\n                      ]\"\n                      @click=\"getCreditApp\"\n                      >Credit Application</a\n                    >\n                  </MenuItem>\n                  <MenuItem v-slot=\"{ active }\">\n                    <a\n                      href=\"#\"\n                      @click=\"getCreditTrans\"\n                      :class=\"[\n                        active ? 'bg-gray-100 text-gray-900' : 'text-gray-700',\n                        'block px-4 py-2 text-sm',\n                      ]\"\n                      >Savings Application</a\n                    >\n                  </MenuItem>\n                </div>\n              </MenuItems>\n            </transition>\n          </Menu>\n        </div>\n        <div class=\"flex-auto\">\n          <p class=\"mt-2 text-sm text-gray-700\">\n            A list of all the Loan Applications in your account including their\n            name, title, email.\n          </p>\n        </div>\n        <div class=\"mt-4 sm:mt-0 sm:ml-16 sm:flex-none\">\n          <button\n            type=\"button\"\n            class=\"inline-flex items-center justify-center rounded-md border border-transparent bg-gray-600 px-4 py-2 text-lg font-bold text-white shadow-sm hover:bg-gray-700 focus:outline-none focus:ring-2 focus:ring-gray-500 focus:ring-offset-2 sm:w-auto\"\n            ref=\"CreateStaff\"\n            @click=\"setIsOpen(true)\"\n          >\n            New Application\n          </button>\n        </div>\n      </div>\n      <form class=\"w-full ml-16 max-w-sm\">\n        <div class=\"flex items-center border-b border-gray-100 py-2\">\n          <input\n            class=\"appearance-none bg-transparent border-none w-full text-gray-700 mr-3 py-1 px-2 leading-tight focus:outline-none\"\n            type=\"text\"\n            placeholder=\"search\"\n            aria-label=\"Full name\"\n          />\n          <button\n            class=\"flex-shrink-0 bg-gray-500 hover:bg-gray-700 border-gray-500 hover:border-gray-700 text-sm border-4 text-white py-1 px-2 rounded\"\n            type=\"button\"\n          >\n            <svg\n              xmlns=\"http://www.w3.org/2000/svg\"\n              class=\"h-3 w-3\"\n              fill=\"none\"\n              viewBox=\"0 0 24 24\"\n              stroke=\"currentColor\"\n              stroke-width=\"2\"\n            >\n              <path\n                stroke-linecap=\"round\"\n                stroke-linejoin=\"round\"\n                d=\"M21 21l-6-6m2-5a7 7 0 11-14 0 7 7 0 0114 0z\"\n              />\n            </svg>\n          </button>\n        </div>\n      </form>\n      <div class=\"mt-8 flex ml-24 flex-col\">\n        <div class=\"-my-2 -mx-4 overflow-x-auto sm:-mx-6 lg:-mx-8\">\n          <div class=\"inline-block min-w-full py-2 align-middle\">\n            <div class=\"overflow-hidden\">\n              <table class=\"min-w-full divide-y divide-gray-300\">\n                <thead class=\"bg-gray-50\">\n                  <tr>\n                    <th\n                      scope=\"col\"\n                      class=\"py-3.5 pl-4 pr-3 text-left text-sm font-semibold text-gray-900 sm:pl-6 lg:pl-8\"\n                    >\n                      Id\n                    </th>\n                    <th\n                      scope=\"col\"\n                      class=\"py-3.5 pl-4 pr-3 text-left text-sm font-semibold text-gray-900 sm:pl-6 lg:pl-8\"\n                    >\n                      Name\n                    </th>\n                    <th\n                      scope=\"col\"\n                      class=\"px-3 py-3.5 text-left text-sm font-semibold text-gray-900\"\n                    >\n                      ID Number\n                    </th>\n                    <th\n                      scope=\"col\"\n                      class=\"px-3 py-3.5 text-left text-sm font-semibold text-gray-900\"\n                    >\n                      Phone Number\n                    </th>\n                    <th\n                      scope=\"col\"\n                      class=\"px-3 py-3.5 text-left text-sm font-semibold text-gray-900\"\n                    >\n                      Email\n                    </th>\n                    <th\n                      scope=\"col\"\n                      class=\"px-3 py-3.5 text-left text-sm font-semibold text-gray-900\"\n                    >\n                      Role\n                    </th>\n                    <th\n                      scope=\"col\"\n                      class=\"relative py-3.5 pl-3 pr-4 sm:pr-6 lg:pr-8\"\n                    >\n                      <span class=\"sr-only\">Edit</span>\n                    </th>\n                  </tr>\n                </thead>\n                <tbody\n                  class=\"divide-y divide-gray-200 bg-white\"\n                  @click=\"setIsTable(true)\"\n                >\n                  <tr v-for=\"(loan, index) in loans\" :key=\"index\">\n                    <td\n                      class=\"whitespace-nowrap py-3.5 pl-4 pr-3 text-sm text-left font-medium text-gray-500 sm:pl-6 lg:pl-8\"\n                      @click=\"setIsTable(true)\"\n                    >\n                      {{ loan.id }}\n                    </td>\n                    <td\n                      class=\"whitespace-nowrap py-3.5 pl-4 pr-3 text-sm text-left font-medium text-gray-900 sm:pl-6 lg:pl-8\"\n                      @click=\"setIsTable(true)\"\n                    >\n                      {{ loan.name }}\n                    </td>\n                    <td\n                      class=\"whitespace-nowrap px-3 py-3.5 text-sm text-left text-gray-500\"\n                      @click=\"setIsTable(true)\"\n                    >\n                      {{ loan.idnumber }}\n                    </td>\n                    <td\n                      class=\"whitespace-nowrap px-3 py-3.5 text-sm text-left text-gray-500\"\n                    >\n                      {{ loan.phonenumber }}\n                    </td>\n                    <td\n                      class=\"whitespace-nowrap px-3 py-3.5 text-sm text-left text-gray-500\"\n                    >\n                      {{ loan.email }}\n                    </td>\n                    <td\n                      class=\"whitespace-nowrap px-3 py-3.5 text-sm text-left text-gray-500\"\n                    >\n                      {{ loan.role }}\n                    </td>\n                    <td\n                      class=\"relative whitespace-nowrap py-4 pl-3 pr-4 text-right text-sm font-medium sm:pr-6 lg:pr-8\"\n                    >\n                      <Menu as=\"div\" class=\"flex-shrink-0 pr-2 py-4\">\n                        <MenuButton\n                          class=\"w-8 h-8 bg-gray inline-flex items-center justify-center text-gray-400 rounded-full hover:text-gray-500\"\n                        >\n                          <span class=\"sr-only\">Open options</span>\n                          <svg\n                            xmlns=\"http://www.w3.org/2000/svg\"\n                            class=\"h-6 w-6\"\n                            fill=\"none\"\n                            viewBox=\"0 0 24 24\"\n                            stroke=\"currentColor\"\n                            stroke-width=\"2\"\n                          >\n                            <path\n                              stroke-linecap=\"round\"\n                              stroke-linejoin=\"round\"\n                              d=\"M12 5v.01M12 12v.01M12 19v.01M12 6a1 1 0 110-2 1 1 0 010 2zm0 7a1 1 0 110-2 1 1 0 010 2zm0 7a1 1 0 110-2 1 1 0 010 2z\"\n                            />\n                          </svg>\n                        </MenuButton>\n                        <transition\n                          enter-active-class=\"transition ease-out duration-100\"\n                          enter-from-class=\"transform opacity-0 scale-95\"\n                          enter-to-class=\"transform opacity-100 scale-100\"\n                          leave-active-class=\"transition ease-in duration-75\"\n                          leave-from-class=\"transform opacity-100 scale-100\"\n                          leave-to-class=\"transform opacity-0 scale-95\"\n                        >\n                          <MenuItems\n                            class=\"z-10 mx-3 origin-top-right absolute right-10 top-3 w-48 mt-1 rounded-md shadow-lg bg-white ring-1 ring-black ring-opacity-5 divide-y divide-gray-200 focus:outline-none\"\n                          >\n                            <div class=\"py-1\">\n                              <MenuItem v-slot=\"{ active }\">\n                                <a\n                                  href=\"#\"\n                                  :class=\"[\n                                    active\n                                      ? 'bg-gray-100 text-green-900'\n                                      : 'text-green-700',\n                                    'block px-4 py-2 text-sm',\n                                  ]\"\n                                >\n                                  Activate Staff</a\n                                >\n                              </MenuItem>\n                              <MenuItem v-slot=\"{ active }\">\n                                <a\n                                  href=\"#\"\n                                  :class=\"[\n                                    active\n                                      ? 'bg-gray-100 text-red-900'\n                                      : 'text-red-700',\n                                    'block px-4 py-2 text-sm',\n                                  ]\"\n                                  >Deactivate Staff</a\n                                >\n                              </MenuItem>\n                            </div>\n                          </MenuItems>\n                        </transition>\n                      </Menu>\n                    </td>\n                  </tr>\n                </tbody>\n              </table>\n            </div>\n          </div>\n        </div>\n      </div>\n      <!-- view staff details -->\n      <div>\n        <TransitionRoot as=\"template\" :show=\"isTable\">\n          <Dialog\n            as=\"div\"\n            :initialFocus=\"checkDetails\"\n            class=\"relative z-10\"\n            :open=\"isTable\"\n            @close=\"setIsTable = false\"\n          >\n            <div class=\"fixed inset-0\" />\n\n            <div class=\"fixed inset-0 overflow-hidden\">\n              <div class=\"absolute inset-0 overflow-hidden\">\n                <div\n                  class=\"pointer-events-none fixed inset-y-0 right-0 flex max-w-full pl-10 sm:pl-16\"\n                >\n                  <TransitionChild\n                    as=\"template\"\n                    enter=\"transform transition ease-in-out duration-500 sm:duration-700\"\n                    enter-from=\"translate-x-full\"\n                    enter-to=\"translate-x-0\"\n                    leave=\"transform transition ease-in-out duration-500 sm:duration-700\"\n                    leave-from=\"translate-x-0\"\n                    leave-to=\"translate-x-full\"\n                  >\n                    <DialogPanel class=\"pointer-events-auto w-screen max-w-2xl\">\n                      <div\n                        class=\"flex h-full flex-col overflow-y-scroll bg-white py-6 shadow-xl\"\n                      >\n                        <div class=\"px-4 sm:px-6\">\n                          <div class=\"flex items-start justify-between\">\n                            <DialogTitle\n                              class=\"text-lg font-medium text-gray-900\"\n                            >\n                              view Staff's details\n                            </DialogTitle>\n                            <div class=\"ml-3 flex h-7 items-center\">\n                              <button\n                                type=\"button\"\n                                class=\"rounded-md bg-white text-gray-400 hover:text-gray-500 focus:outline-none focus:ring-2 focus:ring-gray-500 focus:ring-offset-2\"\n                                @click=\"setIsTable(false)\"\n                              >\n                                <span class=\"sr-only\">Close panel</span>\n                                <XIcon class=\"h-6 w-6\" aria-hidden=\"true\" />\n                              </button>\n                            </div>\n                          </div>\n                        </div>\n                        <div class=\"relative mt-6 flex-1 px-4 sm:px-6\">\n                          <!-- Replace with your content -->\n\n                          <div class=\"absolute inset-0 px-4 sm:px-6\">\n                            <div\n                              class=\"h-full border-2 border-dashed border-gray-200\"\n                              aria-hidden=\"true\"\n                            />\n                          </div>\n                          <!-- /End replace -->\n                        </div>\n                      </div>\n                    </DialogPanel>\n                  </TransitionChild>\n                </div>\n              </div>\n            </div>\n          </Dialog>\n        </TransitionRoot>\n      </div>\n      <!-- create slider  -->\n      <div>\n        <TransitionRoot as=\"template\" :show=\"isOpen\">\n          <Dialog\n            as=\"div\"\n            :initialFocus=\"CreateBorrower\"\n            :open=\"isOpen\"\n            @close=\"setIsOpen\"\n            class=\"relative z-10\"\n          >\n            <div class=\"fixed inset-0\" />\n\n            <div class=\"fixed inset-0 overflow-hidden\">\n              <div class=\"absolute inset-0 overflow-hidden\">\n                <div\n                  class=\"pointer-events-none fixed inset-y-0 right-0 flex max-w-full pl-10 sm:pl-16\"\n                >\n                  <TransitionChild\n                    as=\"template\"\n                    enter=\"transform transition ease-in-out duration-500 sm:duration-700\"\n                    enter-from=\"translate-x-full\"\n                    enter-to=\"translate-x-0\"\n                    leave=\"transform transition ease-in-out duration-500 sm:duration-700\"\n                    leave-from=\"translate-x-0\"\n                    leave-to=\"translate-x-full\"\n                  >\n                    <DialogPanel class=\"pointer-events-auto w-screen max-w-2xl\">\n                      <div\n                        class=\"flex h-full flex-col overflow-y-scroll bg-white py-6 shadow-xl\"\n                      >\n                        <div class=\"px-4 sm:px-6\">\n                          <div class=\"flex items-start justify-between\">\n                            <DialogTitle\n                              class=\"text-lg font-medium text-gray-900\"\n                            >\n                              Create New Application\n                            </DialogTitle>\n                            <div class=\"ml-3 flex h-7 items-center\">\n                              <button\n                                type=\"button\"\n                                class=\"rounded-md bg-white text-gray-400 hover:text-gray-500 focus:outline-none focus:ring-2 focus:ring-gray-500 focus:ring-offset-2\"\n                                @click=\"setIsOpen(false)\"\n                              >\n                                <span class=\"sr-only\">Close panel</span>\n                                <XIcon class=\"h-6 w-6\" aria-hidden=\"true\" />\n                              </button>\n                            </div>\n                          </div>\n                        </div>\n                        <div class=\"relative mt-6 flex-1 px-4 sm:px-6\">\n                          <!-- Replace with your content -->\n                          <div\n                            class=\"bg-white shadow px-4 py-5 sm:rounded-lg lg:p-6\"\n                          >\n                            <div class=\"lg:grid lg:grid-cols-2 lg:gap-6\">\n                              <div class=\"mt-5 md:mt-0 md:col-span-2\">\n                                <form action=\"#\" method=\"POST\">\n                                  <div class