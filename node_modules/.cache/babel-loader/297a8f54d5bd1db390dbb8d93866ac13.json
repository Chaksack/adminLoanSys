{"ast":null,"code":"import Lender from \"../models/Lender\";\nexport default {\n  name: \"LogIn\",\n\n  data() {\n    return {\n      user: Lender,\n      loading: false,\n      message: \"\"\n    };\n  },\n\n  /**\n   * Component getter methods\n   */\n  computed: {\n    loggedIn() {\n      return this.$store.state.StoreAuth.status.loggedIn;\n    }\n\n  },\n\n  /**\n   * Life cycle hooks\n   */\n  created() {\n    if (this.loggedIn) {\n      this.$router.push(\"/dashboard\");\n    }\n  },\n\n  /**\n   * Component methods\n   */\n  methods: {\n    handleLogin() {\n      this.loading = true;\n      this.$validator.validateAll().then(isValid => {\n        if (!isValid) {\n          this.loading = false;\n          return;\n        }\n\n        if (this.user.username && this.user.password) {\n          this.$store.dispatch(\"StoreAuth/login\", this.user).then(() => {\n            this.$router.push(\"/dashboard\");\n          }, error => {\n            this.loading = false;\n            this.message = error.response && error.response.data || error.message || error.toString();\n          });\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA6GA,OAAOA,MAAP,MAAmB,kBAAnB;AAEA,eAAe;EACbC,IAAI,EAAE,OADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAEH,MADD;MAELI,OAAO,EAAE,KAFJ;MAGLC,OAAO,EAAE;IAHJ,CAAP;EAKD,CARY;;EASb;;;EAGAC,QAAQ,EAAE;IACRC,QAAQ,GAAG;MACT,OAAO,KAAKC,MAAL,CAAYC,KAAZ,CAAkBC,SAAlB,CAA4BC,MAA5B,CAAmCJ,QAA1C;IACD;;EAHO,CAZG;;EAiBb;;;EAGAK,OAAO,GAAG;IACR,IAAI,KAAKL,QAAT,EAAmB;MACjB,KAAKM,OAAL,CAAaC,IAAb,CAAkB,YAAlB;IACF;EACD,CAxBY;;EAyBb;;;EAGAC,OAAO,EAAE;IACPC,WAAW,GAAG;MACZ,KAAKZ,OAAL,GAAe,IAAf;MACA,KAAKa,UAAL,CAAgBC,WAAhB,GAA8BC,IAA9B,CAAoCC,OAAD,IAAa;QAC9C,IAAI,CAACA,OAAL,EAAc;UACZ,KAAKhB,OAAL,GAAe,KAAf;UACA;QACF;;QACA,IAAI,KAAKD,IAAL,CAAUkB,QAAV,IAAsB,KAAKlB,IAAL,CAAUmB,QAApC,EAA8C;UAC5C,KAAKd,MAAL,CAAYe,QAAZ,CAAqB,iBAArB,EAAwC,KAAKpB,IAA7C,EAAmDgB,IAAnD,CACE,MAAM;YACJ,KAAKN,OAAL,CAAaC,IAAb,CAAkB,YAAlB;UACD,CAHH,EAIGU,KAAD,IAAW;YACT,KAAKpB,