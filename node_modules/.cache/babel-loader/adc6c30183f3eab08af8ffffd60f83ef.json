{"ast":null,"code":"// const open = ref(true)\nconst people = [{\n  id: 1,\n  name: 'Chak Sack',\n  idnumber: '12345',\n  phonenumber: '09876544321',\n  email: 'Chak.sack@example.com',\n  role: 'SAL1'\n}, {\n  id: 2,\n  name: 'John Doe',\n  idnumber: '12345',\n  phonenumber: '09876544321',\n  email: 'John.Doe@example.com',\n  role: 'SAL1'\n} // More people...\n];\nimport { Menu, MenuButton, MenuItem, MenuItems } from '@headlessui/vue';\nimport { ArrowNarrowLeftIcon, ArrowNarrowRightIcon } from '@heroicons/vue/solid';\nimport Footer from '../../../components/Footer.vue';\nimport LendersSideBar from '../../../components/LendersSideBar.vue';\nimport { ref } from 'vue';\nimport { Dialog, DialogPanel, DialogTitle, TransitionChild, TransitionRoot } from '@headlessui/vue';\nimport { XIcon } from '@heroicons/vue/outline';\nimport { ChevronDownIcon } from '@heroicons/vue/solid'; // import axios from '../../../APIs/axios'\n\nimport axios from 'axios'; // import ListViewStaff from './ListViewStaff.vue'\n\nexport default {\n  // eslint-disable-next-line vue/multi-word-component-names\n  name: 'Lenders',\n  components: {\n    Footer,\n    LendersSideBar,\n    ArrowNarrowLeftIcon,\n    ArrowNarrowRightIcon,\n    Dialog,\n    DialogPanel,\n    DialogTitle,\n    TransitionChild,\n    TransitionRoot,\n    XIcon,\n    Menu,\n    MenuButton,\n    MenuItem,\n    MenuItems,\n    ChevronDownIcon // ListViewStaff,\n\n  },\n\n  created() {\n    axios.get('http://sandbox.pavelon.com/api/users').then(response => {\n      this.users = response.data;\n    }).catch(error => {\n      console.log('There was an error:' + error.response);\n    });\n  },\n\n  setup() {\n    let isOpen = ref(false);\n    let CreateBorrower = ref(null);\n    let isTable = ref(false);\n    let checkDetails = ref(null);\n    return {\n      people,\n      isOpen,\n      CreateBorrower,\n      isTable,\n      checkDetails,\n\n      setIsOpen(value) {\n        isOpen.value = value;\n      },\n\n      setIsTable(value) {\n        isTable.value = value;\n      }\n\n    }; // methods: {\n    //     toggle() {\n    //         this.open = !this.open;\n    //     }\n    // }\n  }\n\n};","map":{"version":3,"mappings":"AAoSA;AAEA,MAAMA,MAAK,GAAI,CACb;EAACC,EAAE,EAAC,CAAJ;EAAOC,IAAI,EAAE,WAAb;EAA0BC,QAAQ,EAAE,OAApC;EAA6CC,WAAW,EAAC,aAAzD;EAAwEC,KAAK,EAAE,uBAA/E;EAAwGC,IAAI,EAAE;AAA9G,CADa,EAEb;EAAEL,EAAE,EAAC,CAAL;EAAQC,IAAI,EAAE,UAAd;EAA0BC,QAAQ,EAAE,OAApC;EAA6CC,WAAW,EAAC,aAAzD;EAAwEC,KAAK,EAAE,sBAA/E;EAAuGC,IAAI,EAAE;AAA7G,CAFa,CAGb;AAHa,CAAf;AAKA,SAASC,IAAT,EAAeC,UAAf,EAA2BC,QAA3B,EAAqCC,SAArC,QAAsD,iBAAtD;AACA,SAASC,mBAAT,EAA8BC,oBAA9B,QAA0D,sBAA1D;AACA,OAAOC,MAAP,MAAmB,gCAAnB;AACA,OAAOC,cAAP,MAA2B,wCAA3B;AACA,SAASC,GAAT,QAAoB,KAApB;AACA,SAASC,MAAT,EAAiBC,WAAjB,EAA8BC,WAA9B,EAA2CC,eAA3C,EAA4DC,cAA5D,QAAkF,iBAAlF;AACA,SAASC,KAAT,QAAsB,wBAAtB;AACA,SAASC,eAAT,QAAgC,sBAAhC,C,CACA;;AACA,OAAOC,KAAP,MAAkB,OAAlB,C,CAGA;;AACA,eAAe;EACf;EACArB,IAAI,EAAE,SAFS;EAGfsB,UAAU,EAAE;IACRX,MADQ;IAERC,cAFQ;IAGRH,mBAHQ;IAIRC,oBAJQ;IAKNI,MALM;IAMRC,WANQ;IAORC,WAPQ;IAQRC,eARQ;IASRC,cATQ;IAURC,KAVQ;IAWRd,IAXQ;IAYRC,UAZQ;IAaRC,QAbQ;IAcRC,SAdQ;IAeRY,eAfQ,CAiBR;;EAjBQ,CAHG;;EAsBfG,OAAO,GAAG;IACNF,KAAK,CAACG,GAAN,CAAU,sCAAV,EACCC,IADD,CACMC,QAAO,IAAK;MAChB,KAAKC,KAAL,GAAaD,QAAQ,CAACE,IAAtB;IACD,CAHD,EAICC,KAJD,CAIOC,KAAI,IAAK;MACdC,OAAO,CAACC,GAAR,CAAY,wBAAwBF,KAAK,CAACJ,QAA1C;IACD,CAND;EAOD,CA9BY;;EAgCfO,KAAK,GAAG;IACJ,IAAIC,MAAK,GAAIrB,GAAG,CAAC,KAAD,CAAhB;IACA,IAAIsB,cAAa,GAAItB,GAAG,CAAC,IAAD,CAAxB;IACA,IAAIuB,OAAM,GAAIvB,GAAG,CAAC,KAAD,CAAjB;IACA,IAAIwB,YAAW,GAAIxB,GAAG,CAAC,IAAD,CAAtB;IAIE,OAAO;MACLf,MADK;MAELoC,MAFK;MAGLC,cAHK;MAILC,OAJK;MAKLC,YALK;;MAMLC,SAAS,CAACC,KAAD,EAAQ;QACbL,MAAM,CAACK,KAAP,GAAeA,KAAf;MACH,CARI;;MASJC,UAAU,CAACD,KAAD,EAAQ;QACfH,OAAO,CAACG,KAAR,GAAgBA,KAAhB;MACH;;IAXI,CAAP,CARE,CAsBJ;IACA;IACA;IACA;IACA;EACH;;AA3Dc,CAAf","names":["people","id","name","idnumber","phonenumber","email","role","Menu","MenuButton","MenuItem","MenuItems","ArrowNarrowLeftIcon","ArrowNarrowRightIcon","Footer","LendersSideBar","ref","Dialog","DialogPanel","DialogTitle","TransitionChild","TransitionRoot","XIcon","ChevronDownIcon","axios","components","created","get","then","response","users","data","catch","error","console","log","setup","isOpen","CreateBorrower","isTable","checkDetails","setIsOpen","value","setIsTable"],"sourceRoot":"","sources":["/Users/chakdahahsackey/pavelonv0.01/src/views/Dashboard.vue"],"sourcesContent":["<template>\n<div>\n<div class=\"flex min-h-screen\">\n<div>\n <LendersSideBar />\n</div>\n\n<!-- create slider  -->\n  <div>\n         <TransitionRoot as=\"template\" :show=\"isOpen\">\n    <Dialog as=\"div\" \n    :initialFocus=\"CreateBorrower\" :open=\"isOpen\" @close=\"setIsOpen\"\n    class=\"relative z-10\" >\n      <div class=\"fixed inset-0\"  />\n\n      <div class=\"fixed inset-0 overflow-hidden\">\n        <div class=\"absolute inset-0 overflow-hidden\">\n          <div class=\"pointer-events-none fixed inset-y-0 right-0 flex max-w-full pl-10 sm:pl-16\">\n            <TransitionChild as=\"template\" enter=\"transform transition ease-in-out duration-500 sm:duration-700\" enter-from=\"translate-x-full\" enter-to=\"translate-x-0\" leave=\"transform transition ease-in-out duration-500 sm:duration-700\" leave-from=\"translate-x-0\" leave-to=\"translate-x-full\">\n              <DialogPanel class=\"pointer-events-auto w-screen max-w-2xl\">\n                <div class=\"flex h-full flex-col overflow-y-scroll bg-white py-6 shadow-xl\">\n                  <div class=\"px-4 sm:px-6\">\n                    <div class=\"flex items-start justify-between\">\n                      <DialogTitle class=\"text-lg font-medium text-gray-900\"> Create Staff </DialogTitle>\n                      <div class=\"ml-3 flex h-7 items-center\">\n                        <button type=\"button\" class=\"rounded-md bg-white text-gray-400 hover:text-gray-500 focus:outline-none focus:ring-2 focus:ring-gray-500 focus:ring-offset-2\" @click=\"setIsOpen(false)\">\n                          <span class=\"sr-only\">Close panel</span>\n                          <XIcon class=\"h-6 w-6\" aria-hidden=\"true\" />\n                        </button>\n                      </div>\n                    </div>\n                  </div>\n                  <div class=\"relative mt-6 flex-1 px-4 sm:px-6\">\n                    <!-- Replace with your content -->\n                      <div class=\"bg-white shadow px-4 py-5 sm:rounded-lg lg:p-6\">\n      <div class=\"lg:grid lg:grid-cols-2 lg:gap-6\">\n        <div class=\"mt-5 md:mt-0 md:col-span-2\">\n          <form action=\"#\" method=\"POST\">\n            <div class=\"grid grid-cols-6 gap-6\">\n              <div class=\"col-span-6 md:col-span-3\">\n                <label for=\"first-name\" class=\"block text-sm font-medium text-gray-700\">First name</label>\n                <input type=\"text\" name=\"first-name\" id=\"first-name\" autocomplete=\"given-name\" class=\"mt-1 focus:ring-gray-500 focus:border-gray-500 block w-full shadow-sm sm:text-sm border-gray-300 rounded-md\" />\n              </div>\n\n              <div class=\"col-span-6 md:col-span-3\">\n                <label for=\"last-name\" class=\"block text-sm font-medium text-gray-700\">Last name</label>\n                <input type=\"text\" name=\"last-name\" id=\"last-name\" autocomplete=\"family-name\" class=\"mt-1 focus:ring-gray-500 focus:border-gray-500 block w-full shadow-sm sm:text-sm border-gray-300 rounded-md\" />\n              </div>\n\n              <div class=\"col-span-6 lg:col-span-4\">\n                <label for=\"email-address\" class=\"block text-sm font-medium text-gray-700\">Email address</label>\n                <input type=\"text\" name=\"email-address\" id=\"email-address\" autocomplete=\"email\" class=\"mt-1 focus:ring-gray-500 focus:border-gray-500 block w-full shadow-sm sm:text-sm border-gray-300 rounded-md\" />\n              </div>\n\n              <!-- <div class=\"col-span-6 sm:col-span-3\">\n                <label for=\"country\" class=\"block text-sm font-medium text-gray-700\">Country</label>\n                <select id=\"country\" name=\"country\" autocomplete=\"country-name\" class=\"mt-1 block w-full py-2 px-3 border border-gray-300 bg-white rounded-md shadow-sm focus:outline-none focus:ring-indigo-500 focus:border-indigo-500 sm:text-sm\">\n                  <option>United States</option>\n                  <option>Canada</option>\n                  <option>Mexico</option>\n                </select>\n              </div> -->\n\n              <div class=\"col-span-6\">\n                <label for=\"street-address\" class=\"block text-sm font-medium text-gray-700\">Street address</label>\n                <input type=\"text\" name=\"street-address\" id=\"street-address\" autocomplete=\"street-address\" class=\"mt-1 focus:ring-gray-500 focus:border-gray-500 block w-full shadow-sm sm:text-sm border-gray-300 rounded-md\" />\n              </div>\n\n              <div class=\"col-span-6 md:col-span-6 lg:col-span-2\">\n                <label for=\"city\" class=\"block text-sm font-medium text-gray-700\">City</label>\n                <input type=\"text\" name=\"city\" id=\"city\" autocomplete=\"address-level2\" class=\"mt-1 focus:ring-gray-500 focus:border-gray-500 block w-full shadow-sm sm:text-sm border-gray-300 rounded-md\" />\n              </div>\n\n              <div class=\"col-span-6 md:col-span-3 lg:col-span-2\">\n                <label for=\"region\" class=\"block text-sm font-medium text-gray-700\">State / Province</label>\n                <input type=\"text\" name=\"region\" id=\"region\" autocomplete=\"address-level1\" class=\"mt-1 focus:ring-gray-500 focus:border-gray-500 block w-full shadow-sm sm:text-sm border-gray-300 rounded-md\" />\n              </div>\n\n              <div class=\"col-span-6 md:col-span-3 lg:col-span-2\">\n                <label for=\"postal-code\" class=\"block text-sm font-medium text-gray-700\">ZIP / Postal code</label>\n                <input type=\"text\" name=\"postal-code\" id=\"postal-code\" autocomplete=\"postal-code\" class=\"mt-1 focus:ring-gray-500 focus:border-gray-500 block w-full shadow-sm sm:text-sm border-gray-300 rounded-md\" />\n              </div>\n              <div class=\"col-span-6 lg:col-span-4\">\n                                      <button type=\"button\" class=\"inline-flex items-center justify-center rounded-md border border-transparent bg-gray-600 px-4 py-2 text-lg font-bold text-white shadow-sm hover:bg-gray-900 focus:outline-none focus:ring-2 focus:ring-gray-500 focus:ring-offset-2 sm:w-auto\" @click=\"setIsOpen(false)\">Create Borrower</button>\n              </div>\n\n            </div>\n          </form>\n        </div>\n      </div>\n    </div>\n                    <!-- <div class=\"absolute inset-0 px-4 sm:px-6\">\n                      <div class=\"h-full border-2 border-dashed border-gray-200\" aria-hidden=\"true\" />\n                    </div> -->\n                    <!-- /End replace -->\n                  </div>\n                </div>\n              </DialogPanel>\n            </TransitionChild>\n          </div>\n        </div>\n      </div>\n    </Dialog>\n  </TransitionRoot>\n    </div>\n\n    <!-- end create slider -->\n\n  \n  <div class=\" relative top-16 right-11 px-10 sm:px-6 lg:px-8 flex-1\">\n    <div class=\"sm:flex sm:items-center\">\n       <div class=\"mt-4 sm:mt-0 sm:ml-11 sm:flex-none\">\n        <Menu>\n      <MenuButton class=\"inline-flex justify-center w-full font-sans font-bold text-2xl rounded-md border-none border-white px-4 py-2 bg-white text-gray-700 hover:bg-gray-200 focus:outline-none \">\n       Applications\n        <ChevronDownIcon class=\"-mr-1 ml-2 h-5 w-5\" aria-hidden=\"true\" />\n      </MenuButton>\n\n    <transition enter-active-class=\"transition ease-out duration-100\" enter-from-class=\"transform opacity-0 scale-95\" enter-to-class=\"transform opacity-100 scale-100\" leave-active-class=\"transition ease-in duration-75\" leave-from-class=\"transform opacity-100 scale-100\" leave-to-class=\"transform opacity-0 scale-95\">\n      <MenuItems class=\"origin-top-left absolute left-11 mt-2 w-56 rounded-md shadow-lg bg-white ring-1 ring-black ring-opacity-5 focus:outline-none\">\n        <div class=\"py-1\">\n          <MenuItem v-slot=\"{ active }\">\n            <a href=\"#\" :class=\"[active ? 'bg-gray-100 text-gray-900' : 'text-gray-700', 'block px-4 py-2 text-sm']\">Credit Application</a>\n          </MenuItem>\n          <MenuItem v-slot=\"{ active }\">\n            <a href=\"#\" :class=\"[active ? 'bg-gray-100 text-gray-900' : 'text-gray-700', 'block px-4 py-2 text-sm']\">Savings Application</a>\n          </MenuItem>\n        </div>\n      </MenuItems>\n    </transition>\n    </Menu>\n      </div>\n      <div class=\"flex-auto\">\n        <p class=\"mt-2 text-sm text-gray-700\">A list of all the Borrowers in your account including their name, title, email.</p>\n      </div>\n      <div class=\"mt-4 sm:mt-0 sm:ml-16 sm:flex-none\">\n        <button type=\"button\" class=\"inline-flex items-center justify-center rounded-md border border-transparent bg-gray-600 px-4 py-2 text-lg font-bold text-white shadow-sm hover:bg-gray-700 focus:outline-none focus:ring-2 focus:ring-gray-500 focus:ring-offset-2 sm:w-auto\" ref=\"CreateStaff\" @click=\"setIsOpen(true)\">New Application</button>\n      </div>\n    </div>\n    <form class=\"w-full ml-16 max-w-sm\">\n  <div class=\"flex items-center border-b border-gray-500 py-2\">\n    <input class=\"appearance-none bg-transparent border-none w-full text-gray-700 mr-3 py-1 px-2 leading-tight focus:outline-none\" type=\"text\" placeholder=\"search\" aria-label=\"Full name\">\n    <button class=\"flex-shrink-0 bg-gray-500 hover:bg-gray-700 border-gray-500 hover:border-gray-700 text-sm border-4 text-white py-1 px-2 rounded\" type=\"button\">\n     <svg xmlns=\"http://www.w3.org/2000/svg\" class=\"h-3 w-3\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\" stroke-width=\"2\">\n  <path stroke-linecap=\"round\" stroke-linejoin=\"round\" d=\"M21 21l-6-6m2-5a7 7 0 11-14 0 7 7 0 0114 0z\" />\n</svg>\n    </button>\n  </div>\n</form>\n    <div class=\"mt-8 flex ml-16 flex-col\">\n      <div class=\"-my-2 -mx-4 overflow-x-auto sm:-mx-6 lg:-mx-8\">\n        <div class=\"inline-block min-w-full py-2 align-middle\">\n          <div class=\"overflow-hidden shadow-sm ring-1 ring-black ring-opacity-5\">\n            <table class=\"min-w-full divide-y divide-gray-300\">\n              <thead class=\"bg-gray-50\">\n                <tr>\n                                    <th scope=\"col\" class=\"py-3.5 pl-4 pr-3 text-left text-sm font-semibold text-gray-900 sm:pl-6 lg:pl-8\">Id</th>\n                  <th scope=\"col\" class=\"py-3.5 pl-4 pr-3 text-left text-sm font-semibold text-gray-900 sm:pl-6 lg:pl-8\">Name</th>\n                  <th scope=\"col\" class=\"px-3 py-3.5 text-left text-sm font-semibold text-gray-900\">ID Number</th>\n                 <th scope=\"col\" class=\"px-3 py-3.5 text-left text-sm font-semibold text-gray-900\">Phone Number</th>\n                  <th scope=\"col\" class=\"px-3 py-3.5 text-left text-sm font-semibold text-gray-900\">Email</th>\n                  <th scope=\"col\" class=\"px-3 py-3.5 text-left text-sm font-semibold text-gray-900\">Role</th>\n                  <th scope=\"col\" class=\"relative py-3.5 pl-3 pr-4 sm:pr-6 lg:pr-8\">\n                    <span class=\"sr-only\">Edit</span>\n                  </th>\n                </tr>\n              </thead>\n              <tbody class=\"divide-y divide-gray-200 bg-white\" @click=\"setIsTable(true)\">\n                <tr v-for=\"person in people\" :key=\"person.email\">\n                                  <td class=\"whitespace-nowrap py-3.5 pl-4 pr-3 text-sm text-left font-medium text-gray-500 sm:pl-6 lg:pl-8\"  @click=\"setIsTable(true)\">{{ person.id }}</td>\n                  <td class=\"whitespace-nowrap py-3.5 pl-4 pr-3 text-sm text-left font-medium text-gray-900 sm:pl-6 lg:pl-8\"  @click=\"setIsTable(true)\">{{ person.name }}</td>\n                  <td class=\"whitespace-nowrap px-3 py-3.5 text-sm text-left text-gray-500\"  @click=\"setIsTable(true)\">{{ person.idnumber }}</td>\n                  <td class=\"whitespace-nowrap px-3 py-3.5 text-sm text-left text-gray-500\">{{ person.phonenumber }}</td>\n                  <td class=\"whitespace-nowrap px-3 py-3.5 text-sm text-left text-gray-500\">{{ person.email }}</td>\n                  <td class=\"whitespace-nowrap px-3 py-3.5 text-sm text-left text-gray-500\">{{ person.role }}</td>\n                  <td class=\"relative whitespace-nowrap py-4 pl-3 pr-4 text-right text-sm font-medium sm:pr-6 lg:pr-8\">\n                     <Menu as=\"div\" class=\"flex-shrink-0 pr-2 py-4\">\n                  <MenuButton class=\"w-8 h-8 bg-gray inline-flex items-center justify-center text-gray-400 rounded-full hover:text-gray-500\">\n                    <span class=\"sr-only\">Open options</span>\n                    <svg xmlns=\"http://www.w3.org/2000/svg\" class=\"h-6 w-6\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\" stroke-width=\"2\">\n  <path stroke-linecap=\"round\" stroke-linejoin=\"round\" d=\"M12 5v.01M12 12v.01M12 19v.01M12 6a1 1 0 110-2 1 1 0 010 2zm0 7a1 1 0 110-2 1 1 0 010 2zm0 7a1 1 0 110-2 1 1 0 010 2z\" />\n</svg>\n                  </MenuButton>\n                  <transition enter-active-class=\"transition ease-out duration-100\" enter-from-class=\"transform opacity-0 scale-95\" enter-to-class=\"transform opacity-100 scale-100\" leave-active-class=\"transition ease-in duration-75\" leave-from-class=\"transform opacity-100 scale-100\" leave-to-class=\"transform opacity-0 scale-95\">\n                    <MenuItems class=\"z-10 mx-3 origin-top-right absolute right-10 top-3 w-48 mt-1 rounded-md shadow-lg bg-white ring-1 ring-black ring-opacity-5 divide-y divide-gray-200 focus:outline-none\">\n                      <div class=\"py-1\">\n                        <MenuItem v-slot=\"{ active }\">\n                          <a href=\"#\" :class=\"[active ? 'bg-gray-100 text-green-900' : 'text-green-700', 'block px-4 py-2 text-sm']\"> Activate Staff</a>\n                        </MenuItem>\n                        <MenuItem v-slot=\"{ active }\">\n                          <a href=\"#\" :class=\"[active ? 'bg-gray-100 text-red-900' : 'text-red-700', 'block px-4 py-2 text-sm']\">Deactivate Staff</a>\n                        </MenuItem>\n                      </div>\n                    </MenuItems>\n                  </transition>\n                </Menu>\n\n    \n                  </td>\n                </tr>\n              </tbody>\n            </table>\n          </div>\n        </div>\n      </div>\n      \n    </div>\n\n    <!-- view staff details -->\n    <div>\n         <TransitionRoot as=\"template\" :show=\"isTable\">\n    <Dialog as=\"div\" \n    :initialFocus=\"checkDetails\" class=\"relative z-10\" :open=\"isTable\" @close=\"setIsTable = false\">\n      <div class=\"fixed inset-0\" />\n\n      <div class=\"fixed inset-0 overflow-hidden\">\n        <div class=\"absolute inset-0 overflow-hidden\">\n          <div class=\"pointer-events-none fixed inset-y-0 right-0 flex max-w-full pl-10 sm:pl-16\">\n            <TransitionChild as=\"template\" enter=\"transform transition ease-in-out duration-500 sm:duration-700\" enter-from=\"translate-x-full\" enter-to=\"translate-x-0\" leave=\"transform transition ease-in-out duration-500 sm:duration-700\" leave-from=\"translate-x-0\" leave-to=\"translate-x-full\">\n              <DialogPanel class=\"pointer-events-auto w-screen max-w-2xl\">\n                <div class=\"flex h-full flex-col overflow-y-scroll bg-white py-6 shadow-xl\">\n                  <div class=\"px-4 sm:px-6\">\n                    <div class=\"flex items-start justify-between\">\n                      <DialogTitle class=\"text-lg font-medium text-gray-900\"> view Staff's details </DialogTitle>\n                      <div class=\"ml-3 flex h-7 items-center\">\n                        <button type=\"button\" class=\"rounded-md bg-white text-gray-400 hover:text-gray-500 focus:outline-none focus:ring-2 focus:ring-gray-500 focus:ring-offset-2\" @click=\"setIsTable(false)\">\n                          <span class=\"sr-only\">Close panel</span>\n                          <XIcon class=\"h-6 w-6\" aria-hidden=\"true\" />\n                        </button>\n                      </div>\n                    </div>\n                  </div>\n                  <div class=\"relative mt-6 flex-1 px-4 sm:px-6\"> \n                    <!-- Replace with your content -->\n                    \n           \n                    <div class=\"absolute inset-0 px-4 sm:px-6\">\n                      <div class=\"h-full border-2 border-dashed border-gray-200\" aria-hidden=\"true\" />\n                    </div>\n                    <!-- /End replace -->\n                   </div>\n                </div>\n              </DialogPanel>\n            </TransitionChild>\n          </div>\n        </div>\n      </div>\n    </Dialog>\n  </TransitionRoot>\n    </div> \n\n\n<!-- pagination -->\n    <div>\n        <nav class=\"border-t border-gray-200 px-4 flex items-center justify-between sm:px-0\">\n    <div class=\"-mt-px ml-16 w-0 flex-1 flex\">\n      <a href=\"#\" class=\"border-t-2 border-transparent pt-4 pr-1 inline-flex items-center text-sm font-medium text-gray-500 hover:text-gray-700 hover:border-gray-300\">\n        <ArrowNarrowLeftIcon class=\"mr-3 h-5 w-5 text-gray-400\" aria-hidden=\"true\" />\n        Previous\n      </a>\n    </div>\n    <div class=\"hidden md:-mt-px md:flex\">\n      <a href=\"#\" class=\"border-transparent text-gray-500 hover:text-gray-700 hover:border-gray-300 border-t-2 pt-4 px-4 inline-flex items-center text-sm font-medium\"> 1 </a>\n      <!-- Current: \"border-indigo-500 text-indigo-600\", Default: \"border-transp