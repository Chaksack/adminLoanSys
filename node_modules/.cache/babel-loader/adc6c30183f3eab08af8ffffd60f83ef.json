headlessui/vue'\nimport { ArrowNarrowLeftIcon, ArrowNarrowRightIcon } from '@heroicons/vue/solid'\nimport Footer from '../../../components/Footer.vue'\nimport LendersSideBar from '../../../components/LendersSideBar.vue'\nimport { ref } from 'vue'\nimport { Dialog, DialogPanel, DialogTitle, TransitionChild, TransitionRoot } from '@headlessui/vue'\nimport { XIcon } from '@heroicons/vue/outline'\nimport { ChevronDownIcon } from '@heroicons/vue/solid'\n// import axios from '../../../APIs/axios'\nimport axios from 'axios'\n\n\n// import ListViewStaff from './ListViewStaff.vue'\nexport default {\n// eslint-disable-next-line vue/multi-word-component-names\nname: 'Lenders',\ncomponents: {\n    Footer,\n    LendersSideBar,\n    ArrowNarrowLeftIcon,\n    ArrowNarrowRightIcon,\n      Dialog,\n    DialogPanel,\n    DialogTitle,\n    TransitionChild,\n    TransitionRoot,\n    XIcon,\n    Menu,\n    MenuButton,\n    MenuItem,\n    MenuItems,\n    ChevronDownIcon,\n    \n    // ListViewStaff,\n},\ncreated() {\n    axios.get('http://sandbox.pavelon.com/api/users')\n    .then(response => {\n      this.users = response.data\n    })\n    .catch(error => {\n      console.log('There was an error:' + error.response)\n    })\n  },\n\nsetup() {\n    let isOpen = ref(false);\n    let CreateBorrower = ref(null);\n    let isTable = ref(false);\n    let checkDetails = ref(null);\n\n\n\n      return {\n        people,\n        isOpen,\n        CreateBorrower,\n        isTable,\n        checkDetails,\n        setIsOpen(value) {\n            isOpen.value = value;\n        },\n         setIsTable(value) {\n            isTable.value = value;\n        },\n    };\n    \n    // methods: {\n    //     toggle() {\n    //         this.open = !this.open;\n    //     }\n    // }\n},\n};\n</script>\n\n<style>\n\n</style>"]},"metadata":{},"sourceType":"module"}