{"ast":null,"code":"import { ref } from \"vue\";\nimport SideBar from \"../components/SideBar\";\nimport Footer from \"../components/Footer\"; // import { Menu, MenuButton, MenuItem, MenuItems } from \"@headlessui/vue\";\n\nimport axios from \"axios\";\nimport { ArrowNarrowLeftIcon, ArrowNarrowRightIcon } from \"@heroicons/vue/solid\";\nimport { Dialog, DialogPanel, DialogTitle, TransitionChild, TransitionRoot } from \"@headlessui/vue\";\nimport { XIcon } from \"@heroicons/vue/outline\";\nimport LocalStorageService from \"../APIs/token\";\nconst tabs = [{\n  name: \"Credit Application\",\n  current: true\n}, {\n  name: \"Savings Application\",\n  current: false\n}];\nexport default {\n  // eslint-disable-next-line vue/multi-word-component-names\n  name: \"Dashboard\",\n  components: {\n    SideBar,\n    Footer,\n    ArrowNarrowLeftIcon,\n    ArrowNarrowRightIcon,\n    Dialog,\n    DialogPanel,\n    DialogTitle,\n    TransitionChild,\n    TransitionRoot,\n    XIcon // Menu,\n    // MenuButton,\n    // MenuItem,\n    // MenuItems,\n\n  },\n\n  data() {\n    return {\n      credit: [],\n      transaction: [],\n      tableData: [],\n      index: \"Credit\"\n    };\n  },\n\n  methods: {\n    showData(e) {\n      console.log(e);\n\n      if (e == \"Credit\") {\n        this.index = \"Transaction\";\n        this.tableData = this.transaction;\n      } else {\n        this.index = \"Credit\";\n        this.tableData = this.credit;\n        console.log(this.tableData);\n      }\n    },\n\n    getCreditApp() {\n      const creditAPI = axios.create({\n        baseURL: \"http://sandbox.pavelon.com\",\n        headers: {\n          Authorization: \"Bearer \" + LocalStorageService.getAccessToken(),\n          \"x-api-key\": \"x64cRjHA3240SSdfdk\",\n          \"x-api-version\": \"v3\",\n          \"xbs-token\": LocalStorageService.getXbsToken()\n        }\n      }); // extAPI.interceptors.request.use((config) => {\n      //   config.headers[\"Authorization\"] =\n      //     \"Bearer \" + localStorage.getItem(\"token\");\n      // });\n\n      creditAPI.get(\"/api/credit\").then(response => {\n        // this.getCreditApp = response.data.pageItems;\n        this.credit = response.data.pageItems;\n        this.tableData = this.credit;\n        console.log(response.data.pageItems);\n      }).catch(error => {\n        console.log(\"There was an error:\", error.data);\n      });\n    },\n\n    getCreditTrans() {\n      const transAPI = axios.create({\n        baseURL: \"http://sandbox.pavelon.com\",\n        headers: {\n          Authorization: \"Bearer \" + LocalStorageService.getAccessToken(),\n          \"x-api-key\": \"x64cRjHA3240SSdfdk\",\n          \"x-api-version\": \"v3\",\n          \"xbs-token\": LocalStorageService.getXbsToken()\n        }\n      });\n      transAPI.get(\" /api/savings\").then(response => {\n        this.getCreditTrans = response.data.pageItems;\n        console.log(response.data);\n      }).catch(error => {\n        console.log(\"There was an error:\", error.data);\n      });\n    } //   return {\n    //   handleSubmit,\n    // }\n\n\n  },\n\n  mounted() {\n    this.getCreditApp();\n    this.getCreditTrans();\n  },\n\n  setup() {\n    let isOpen = ref(false);\n    let CreateBorrower = ref(null);\n    let isTable = ref(false);\n    let checkDetails = ref(null);\n    return {\n      tabs,\n      isOpen,\n      CreateBorrower,\n      isTable,\n      checkDetails,\n\n      setIsOpen(value) {\n        isOpen.value = value;\n      },\n\n      setIsTable(value) {\n        isTable.value = value;\n      }\n\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAqrBA,SAASA,GAAT,QAAoB,KAApB;AACA,OAAOC,OAAP,MAAoB,uBAApB;AACA,OAAOC,MAAP,MAAmB,sBAAnB,C,CACA;;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,SACEC,mBADF,EAEEC,oBAFF,QAGO,sBAHP;AAIA,SACEC,MADF,EAEEC,WAFF,EAGEC,WAHF,EAIEC,eAJF,EAKEC,cALF,QAMO,iBANP;AAOA,SAASC,KAAT,QAAsB,wBAAtB;AACA,OAAOC,mBAAP,MAAgC,eAAhC;AACA,MAAMC,IAAG,GAAI,CACX;EAAEC,IAAI,EAAE,oBAAR;EAA8BC,OAAO,EAAE;AAAvC,CADW,EAEX;EAAED,IAAI,EAAE,qBAAR;EAA+BC,OAAO,EAAE;AAAxC,CAFW,CAAb;AAIA,eAAe;EACb;EACAD,IAAI,EAAE,WAFO;EAGbE,UAAU,EAAE;IACVf,OADU;IAEVC,MAFU;IAGVE,mBAHU;IAIVC,oBAJU;IAKVC,MALU;IAMVC,WANU;IAOVC,WAPU;IAQVC,eARU;IASVC,cATU;IAUVC,KAVU,CAWV;IACA;IACA;IACA;;EAdU,CAHC;;EAmBbM,IAAI,GAAG;IACL,OAAO;MACLC,MAAM,EAAE,EADH;MAELC,WAAW,EAAE,EAFR;MAGLC,SAAS,EAAE,EAHN;MAILC,KAAK,EAAE;IAJF,CAAP;EAMD,CA1BY;;EA4BbC,OAAO,EAAE;IACPC,QAAQ,CAACC,CAAD,EAAI;MACVC,OAAO,CAACC,GAAR,CAAYF,CAAZ;;MACA,IAAIA,KAAK,QAAT,EAAmB;QACjB,KAAKH,KAAL,GAAa,aAAb;QACA,KAAKD,SAAL,GAAiB,KAAKD,WAAtB;MACF,CAHA,MAGO;QACL,KAAKE,KAAL,GAAa,QAAb;QACA,KAAKD,SAAL,GAAiB,KAAKF,MAAtB;QACAO,OAAO,CAACC,GAAR,CAAY,KAAKN,SAAjB;MACF;IACD,CAXM;;IAYPO,YAAY,GAAG;MACb,MAAMC,SAAQ,GAAIzB,KAAK,CAAC0B,MAAN,CAAa;QAC7BC,OAAO,EAAE,4BADoB;QAE7BC,OAAO,EAAE;UACPC,aAAa,EAAE,YAAYpB,mBAAmB,CAACqB,cAApB,EADpB;UAEP,aAAa,oBAFN;UAGP,iBAAiB,IAHV;UAIP,aAAarB,mBAAmB,CAACsB,WAApB;QAJN;MAFoB,CAAb,CAAlB,CADa,CAWb;MACA;MACA;MACA;;MAEAN,SAAQ,CACLO,GADH,CACO,aADP,EAEGC,IAFH,CAESC,QAAD,IAAc;QAClB;QACA,KAAKnB,MAAL,GAAcmB,QAAQ,CAACpB,IAAT,CAAcqB,SAA5B;QACA,KAAKlB,SAAL,GAAiB,KAAKF,MAAtB;QACAO,OAAO,CAACC,GAAR,CAAYW,QAAQ,CAACpB,IAAT,CAAcqB,SAA1B;MACD,CAPH,EAQGC,KARH,CAQUC,KAAD,IAAW;QAChBf,OAAO,CAACC,GAAR,CAAY,qBAAZ,EAAmCc,KAAK,CAACvB,IAAzC;MACD,CAVH;IAWD,CAvCM;;IAwCPwB,cAAc,GAAG;MACf,MAAMC,QAAO,GAAIvC,KAAK,CAAC0B,MAAN,CAAa;QAC5BC,OAAO,EAAE,4BADmB;QAE5BC,OAAO,EAAE;UACPC,aAAa,EAAE,YAAYpB,mBAAmB,CAACqB,cAApB,EADpB;UAEP,aAAa,oBAFN;UAGP,iBAAiB,IAHV;UAIP,aAAarB,mBAAmB,CAACsB,WAApB;QAJN;MAFmB,CAAb,CAAjB;MASAQ,QAAO,CACJP,GADH,CACO,eADP,EAEGC,IAFH,CAESC,QAAD,IAAc;QAClB,KAAKI,cAAL,GAAsBJ,QAAQ,CAACpB,IAAT,CAAcqB,SAApC;QACAb,OAAO,CAACC,GAAR,CAAYW,QAAQ,CAACpB,IAArB;MACD,CALH,EAMGsB,KANH,CAMUC,KAAD,IAAW;QAChBf,OAAO,CAACC,GAAR,CAAY,qBAAZ,EAAmCc,KAAK,CAACvB,IAAzC;MACD,CARH;IASD,CA3DM,CA4DP;IACA;IACA;;;EA9DO,CA5BI;;EA4Fb0B,OAAO,GAAG;IACR,KAAKhB,YAAL;IACA,KAAKc,cAAL;EACD,CA/FY;;EAgGbG,KAAK,GAAG;IACN,IAAIC,MAAK,GAAI7C,GAAG,CAAC,KAAD,CAAhB;IACA,IAAI8C,cAAa,GAAI9C,GAAG,CAAC,IAAD,CAAxB;IACA,IAAI+C,OAAM,GAAI/C,GAAG,CAAC,KAAD,CAAjB;IACA,IAAIgD,YAAW,GAAIhD,GAAG,CAAC,IAAD,CAAtB;IAEA,OAAO;MACLa,IADK;MAELgC,MAFK;MAGLC,cAHK;MAILC,OAJK;MAKLC,YALK;;MAMLC,SAAS,CAACC,KAAD,EAAQ;QACfL,MAAM,CAACK,KAAP,GAAeA,KAAf;MACD,CARI;;MASLC,UAAU,CAACD,KAAD,EAAQ;QAChBH,OAAO,CAACG,KAAR,GAAgBA,KAAhB;MACD;;IAXI,CAAP;EAaD;;AAnHY,CAAf","names":["ref","SideBar","Footer","axios","ArrowNarrowLeftIcon","ArrowNarrowRightIcon","Dialog","DialogPanel","DialogTitle","TransitionChild","TransitionRoot","XIcon","LocalStorageService","tabs","name","current","components","data","credit","transaction","tableData","index","methods","showData","e","console","log","getCreditApp","creditAPI","create","baseURL","headers","Authorization","getAccessToken","getXbsToken","get","then","response","pageItems","catch","error","getCreditTrans","transAPI","mounted","setup","isOpen","CreateBorrower","isTable","checkDetails","setIsOpen","value","setIsTable"],"sourceRoot":"","sources":["/Users/chakdahahsackey/pavelonv0.01/src/views/Dashboard.vue"],"sourcesContent":["/* eslint-disable no-undef */\n<template>\n  <div class=\"flex min-h-full\">\n    <div>\n      <SideBar />\n    </div>\n    <div class=\"relative top-16 right-11 px-10 sm:px-6 lg:px-8 flex-1\">\n      <div class=\"sm:flex sm:items-center\">\n        <div class=\"mt-4 sm:mt-0 sm:ml-11 sm:flex-none\">\n          <h3\n            class=\"inline-flex justify-center w-full font-sans font-bold text-4xl text-gray-700\"\n          >\n            Applications\n          </h3>\n        </div>\n        <div class=\"flex-auto\">\n          <p class=\"mt-2 text-sm text-gray-700\">\n            A list of all the Loan Applications in your account including their\n            name, title, email.\n          </p>\n        </div>\n        <div class=\"mt-4 sm:mt-0 sm:ml-16 sm:flex-none\">\n          <button\n            type=\"button\"\n            class=\"inline-flex items-center justify-center rounded-md border border-transparent bg-gray-600 px-4 py-2 text-lg font-bold text-white shadow-sm hover:bg-gray-700 focus:outline-none focus:ring-2 focus:ring-gray-500 focus:ring-offset-2 sm:w-auto\"\n            ref=\"CreateStaff\"\n            @click=\"setIsOpen(true)\"\n          >\n            New Application\n          </button>\n        </div>\n      </div>\n      <form class=\"w-full ml-16 max-w-sm\">\n        <div class=\"flex items-center border-b border-gray-100 py-2\">\n          <input\n            class=\"appearance-none bg-transparent border-none w-full text-gray-700 mr-3 py-1 px-2 leading-tight focus:outline-none\"\n            type=\"text\"\n            placeholder=\"search\"\n            aria-label=\"Full name\"\n          />\n          <button\n            class=\"flex-shrink-0 bg-gray-500 hover:bg-gray-700 border-gray-500 hover:border-gray-700 text-sm border-4 text-white py-1 px-2 rounded\"\n            type=\"button\"\n          >\n            <svg\n              xmlns=\"http://www.w3.org/2000/svg\"\n              class=\"h-3 w-3\"\n              fill=\"none\"\n              viewBox=\"0 0 24 24\"\n              stroke=\"currentColor\"\n              stroke-width=\"2\"\n            >\n              <path\n                stroke-linecap=\"round\"\n                stroke-linejoin=\"round\"\n                d=\"M21 21l-6-6m2-5a7 7 0 11-14 0 7 7 0 0114 0z\"\n              />\n            </svg>\n          </button>\n        </div>\n      </form>\n      <div>\n        <div class=\"sm:hidden\">\n          <label for=\"tabs\" class=\"sr-only\">Select a tab</label>\n          <!-- Use an \"onChange\" listener to redirect the user to the selected tab URL. -->\n          <select\n            id=\"tabs\"\n            name=\"tabs\"\n            class=\"block w-full pl-3 pr-10 py-2 text-base border-gray-300 focus:outline-none focus:ring-indigo-500 focus:border-indigo-500 sm:text-sm rounded-md\"\n          >\n            <option v-for=\"tab in tabs\" :key=\"tab.name\" :selected=\"tab.current\">\n              {{ tab.name }}\n            </option>\n          </select>\n        </div>\n        <div class=\"hidden sm:block\">\n          <div class=\"border-b border-gray-200\">\n            <nav class=\"-mb-px flex space-x-8\" aria-label=\"Tabs\">\n              <a\n                v-for=\"tab in tabs\"\n                :key=\"tab.name\"\n                @click=\"showData(tab.name)\"\n                :class=\"[\n                  tab.current\n                    ? 'border-indigo-500 text-indigo-600'\n                    : 'border-transparent text-gray-500 hover:text-gray-700 hover:border-gray-200',\n                  'whitespace-nowrap flex py-4 px-1 border-b-2 font-medium text-sm',\n                ]\"\n              >\n                {{ tab.name }}\n                <span\n                  v-if=\"tab.count\"\n                  :class=\"[\n                    tab.current\n                      ? 'bg-indigo-100 text-indigo-600'\n                      : 'bg-gray-100 text-gray-900',\n                    'hidden ml-3 py-0.5 px-2.5 rounded-full text-xs font-medium md:inline-block',\n                  ]\"\n                  >{{ tab.count }}</span\n                >\n              </a>\n            </nav>\n          </div>\n        </div>\n      </div>\n      <div class=\"mt-8 flex ml-24 flex-col\">\n        <div class=\"-my-2 -mx-4 overflow-x-auto sm:-mx-6 lg:-mx-8\">\n          <div class=\"inline-block min-w-full py-2 align-middle\">\n            <div class=\"overflow-hidden\">\n              <table class=\"min-w-full divide-y divide-gray-300\">\n                <thead class=\"bg-gray-50\">\n                  <tr>\n                    <th\n                      scope=\"col\"\n                      class=\"py-3.5 pl-4 pr-3 text-left text-sm font-semibold text-gray-900 sm:pl-6 lg:pl-8\"\n                    >\n                      Id\n                    </th>\n                    <th\n                      scope=\"col\"\n                      class=\"py-3.5 pl-4 pr-3 text-left text-sm font-semibold text-gray-900 sm:pl-6 lg:pl-8\"\n                    >\n                      Account No\n                    </th>\n                    <th\n                      scope=\"col\"\n                      class=\"px-3 py-3.5 text-left text-sm font-semibold text-gray-900\"\n                    >\n                      Name\n                    </th>\n                    <th\n                      scope=\"col\"\n                      class=\"px-3 py-3.5 text-left text-sm font-semibold text-gray-900\"\n                    >\n                      Client ID\n                    </th>\n                    <th\n                      scope=\"col\"\n                      class=\"px-3 py-3.5 text-left text-sm font-semibold text-gray-900\"\n                    >\n                      Client Office ID\n                    </th>\n                    <th\n                      scope=\"col\"\n                      class=\"px-3 py-3.5 text-left text-sm font-semibold text-gray-900\"\n                    >\n                      Principal Amount\n                    </th>\n                    <!-- <th\n                      scope=\"col\"\n                      class=\"relative py-3.5 pl-3 pr-4 sm:pr-6 lg:pr-8\"\n                    >\n                      <span class=\"sr-only\">Edit</span>\n                    </th> -->\n                  </tr>\n                </thead>\n                <tbody\n                  class=\"divide-y divide-gray-200 bg-white\"\n                  @click=\"setIsTable(true)\"\n                >\n                  <tr v-for=\"(credit, index) in tableData\" :key=\"index\">\n                    <td\n                      class=\"whitespace-nowrap py-3.5 pl-4 pr-3 text-sm text-left font-medium text-gray-500 sm:pl-6 lg:pl-8\"\n                      @click=\"setIsTable(true)\"\n                    >\n                      {{ credit.id }}\n                    </td>\n                    <td\n                      class=\"whitespace-nowrap py-3.5 pl-4 pr-3 text-sm text-left font-medium text-gray-900 sm:pl-6 lg:pl-8\"\n                      @click=\"setIsTable(true)\"\n                    >\n                      {{ credit.accountNo }}\n                    </td>\n                    <td\n                      class=\"whitespace-nowrap px-3 py-3.5 text-sm text-left text-gray-500\"\n                      @click=\"setIsTable(true)\"\n                    >\n                      {{ credit.clientName }}\n                    </td>\n                    <td\n                      class=\"whitespace-nowrap px-3 py-3.5 text-sm text-left text-gray-500\"\n                      @click=\"setIsTable(true)\"\n                    >\n                      {{ credit.clientId }}\n                    </td>\n                    <td\n                      class=\"whitespace-nowrap px-3 py-3.5 text-sm text-left text-gray-500\"\n                      @click=\"setIsTable(true)\"\n                    >\n                      {{ credit.clientOfficeId }}\n                    </td>\n                    <td\n                      class=\"whitespace-nowrap px-3 py-3.5 text-sm text-left text-gray-500\"\n                      @click=\"setIsTable(true)\"\n                    >\n                      {{ credit.approvedPrincipal }}\n                    </td>\n                  </tr>\n                </tbody>\n              </table>\n            </div>\n          </div>\n        </div>\n      </div>\n      <!-- view staff details -->\n      <div>\n        <TransitionRoot as=\"template\" :show=\"isTable\">\n          <Dialog\n            as=\"div\"\n            :initialFocus=\"checkDetails\"\n            class=\"relative z-10\"\n            :open=\"isTable\"\n            @close=\"setIsTable = false\"\n          >\n            <div class=\"fixed inset-0\" />\n\n            <div class=\"fixed inset-0 overflow-hidden\">\n              <div class=\"absolute inset-0 overflow-hidden\">\n                <div\n                  class=\"pointer-events-none fixed inset-y-0 right-0 flex max-w-full pl-10 sm:pl-16\"\n                >\n                  <TransitionChild\n                    as=\"template\"\n                    enter=\"transform transition ease-in-out duration-500 sm:duration-700\"\n                    enter-from=\"translate-x-full\"\n                    enter-to=\"translate-x-0\"\n                    leave=\"transform transition ease-in-out duration-500 sm:duration-700\"\n                    leave-from=\"translate-x-0\"\n                    leave-to=\"translate-x-full\"\n                  >\n                    <DialogPanel class=\"pointer-events-auto w-screen max-w-2xl\">\n                      <div\n                        class=\"flex h-full flex-col overflow-y-scroll bg-white py-6 shadow-xl\"\n                      >\n                        <div class=\"px-4 sm:px-6\">\n                          <div class=\"flex items-start justify-between\">\n                            <DialogTitle\n                              class=\"text-lg font-medium text-gray-900\"\n                            >\n                              view Application details\n                            </DialogTitle>\n                            <div class=\"ml-3 flex h-7 items-center\">\n                              <button\n                                type=\"button\"\n                                class=\"rounded-md bg-white text-gray-400 hover:text-gray-500 focus:outline-none focus:ring-2 focus:ring-gray-500 focus:ring-offset-2\"\n                                @click=\"setIsTable(false)\"\n                              >\n                                <span class=\"sr-only\">Close panel</span>\n                                <XIcon class=\"h-6 w-6\" aria-hidden=\"true\" />\n                              </button>\n                            </div>\n                          </div>\n                        </div>\n                        <div class=\"relative mt-6 flex-1 px-4 sm:px-6\">\n                          <!-- Replace with your content -->\n\n                          <div\n                            class=\"bg-white shadow px-4 py-5 sm:rounded-lg lg:p-6\"\n                          >\n                            <div class=\"lg:grid lg:grid-cols-2 lg:gap-6\">\n                              <div class=\"mt-5 md:mt-0 md:col-span-2\">\n                                <form>\n                                  <div class=\"grid grid-cols-6 gap-6\">\n                                    <div class=\"col-span-6 md:col-span-3\">\n                                      <label\n                                        for=\"clientId\"\n                                        class=\"block text-sm font-medium text-gray-700\"\n                                      ></label>\n                                      <input\n                                        disabled\n                                        type=\"number\"\n                                        name=\"number\"\n                                        id=\"number\"\n                                        autocomplete=\"number\"\n                                        class=\"mt-1 focus:ring-gray-500 focus:border-gray-500 block w-full shadow-sm sm:text-sm border-gray-300 rounded-md\"\n                                      />\n                                    </div>\n                                    <div class=\"col-span-6 md:col-span-3\">\n                                      <label\n                                        for=\"full-name\"\n                                        class=\"block text-sm font-medium text-gray-700\"\n                                        >Client Name</label\n                                      >\n                                      <input\n                                        disabled\n                                        type=\"text\"\n                                        name=\"fullname\"\n                                        id=\"fullname\"\n                                        autocomplete=\"name\"\n                                        class=\"mt-1 focus:ring-gray-500 focus:border-gray-500 block w-full shadow-sm sm:text-sm border-gray-300 rounded-md\"\n                                      />\n                                    </div>\n\n                                    <div class=\"col-span-6 md:col-span-3\">\n                                      <label\n                                        for=\"accountNo\"\n                                        class=\"block text-sm font-medium text-gray-700\"\n                                      ></label>\n                                      <input\n                                        disabled\n                                        type=\"number\"\n                                        name=\"accountNo\"\n                                        id=\"accountNo\"\n                                        autocomplete=\"accountNo\"\n                                        class=\"mt-1 focus:ring-gray-500 focus:border-gray-500 block w-full shadow-sm sm:text-sm border-gray-300 rounded-md\"\n                                      />\n                                    </div>\n\n                                    <div class=\"col-span-6 md:col-span-3\">\n                                      <label\n                                        for=\"emailaddress\"\n                                        class=\"block text-sm font-medium text-gray-700\"\n                                      ></label>\n                                      <input\n                                        type=\"text\"\n                                        name=\"emailaddress\"\n                                        id=\"emailaddress\"\n                                        autocomplete=\"email\"\n                                        class=\"mt-1 focus:ring-gray-500 focus:border-gray-500 block w-full shadow-sm sm:text-sm border-gray-300 rounded-md\"\n                                      />\n                                    </div>\n                                    <div class=\"col-span-6 md:col-span-3\">\n                                      <label\n                                        for=\"mobilenumber\"\n                                        class=\"block text-sm font-medium text-gray-700\"\n                                        >Mobile Number</label\n                                      >\n                                      <input\n                                        type=\"number\"\n                                        name=\"mobilenumber\"\n                                        id=\"mobilenumber\"\n                                        autocomplete=\"number\"\n                                        class=\"mt-1 focus:ring-gray-500 focus:border-gray-500 block w-full shadow-sm sm:text-sm border-gray-300 rounded-md\"\n                                      />\n                                    </div>\n\n                                    <div class=\"col-span-6 lg:col-span-6\">\n                                      <button\n                                        type=\"button\"\n                                        class=\"mx-2 inline-flex items-center justify-center rounded-md border border-transparent bg-green-600 px-4 py-2 text-lg font-bold text-white shadow-sm hover:bg-green-900 focus:outline-none focus:ring-2 focus:ring-green-500 focus:ring-offset-2 sm:w-auto\"\n                                        @click.prevent=\"createBorrower\"\n                                      >\n                                        Approve\n                                      </button>\n                                      <button\n                                        type=\"button\"\n                                        class=\"mx-2 inline-flex items-center justify-center rounded-md border border-transparent bg-red-600 px-4 py-2 text-lg font-bold text-white shadow-sm hover:bg-red-900 focus:outline-none focus:ring-2 focus:ring-red-500 focus:ring-offset-2 sm:w-auto\"\n                                        @click.prevent=\"createBorrower\"\n                                      >\n                                        Reject\n                                      </button>\n                                      <button\n                                        type=\"button\"\n                                        class=\"mx-2 inline-flex items-center justify-center rounded-md border border-transparent bg-gray-600 px-4 py-2 text-lg font-bold text-white shadow-sm hover:bg-gray-900 focus:outline-none focus:ring-2 focus:ring-gray-500 focus:ring-offset-2 sm:w-auto\"\n                                        @click.prevent=\"createBorrower\"\n                                      >\n                                        Close\n                                      </button>\n                                    </div>\n                                  </div>\n                                </form>\n                              </div>\n                            </div>\n                          </div>\n\n                          <div class=\"absolute inset-0 px-4 sm:px-6\">\n                            <div\n                              class=\"h-full border-2 border-dashed border-gray-200\"\n                              aria-hidden=\"true\"\n                            />\n                          </div>\n                          <div class=\"absolute inset-0 px-4 sm:px-6\">\n                            <div\n                              class=\"h-full border-2 border-dashed border-gray-200\"\n                              aria-hidden=\"true\"\n                            />\n                          </div>\n                          <!-- /End replace -->\n                        </div>\n                      </div>\n                    </DialogPanel>\n                  </TransitionChild>\n                </div>\n              </div>\n            </div>\n          </Dialog>\n        </TransitionRoot>\n      </div>\n      <!-- create slider  -->\n      <div>\n        <TransitionRoot as=\"template\" :show=\"isOpen\">\n          <Dialog\n            as=\"div\"\n            :initialFocus=\"CreateBorrower\"\n            :open=\"isOpen\"\n            @close=\"setIsOpen\"\n            class=\"relative z-10\"\n          >\n            <div class=\"fixed inset-0\" />\n\n            <div class=\"fixed inset-0 overflow-hidden\">\n              <div class=\"absolute inset-0 overflow-hidden\">\n                <div\n                  class=\"pointer-events-none fixed inset-y-0 right-0 flex max-w-full pl-10 sm:pl-16\"\n                >\n                  <TransitionChild\n                    as=\"template\"\n                    enter=\"transform transition ease-in-out duration-500 sm:duration-700\"\n                    enter-from=\"translate-x-full\"\n                    enter-to=\"translate-x-0\"\n                    leave=\"transform transition ease-in-out duration-500 sm:duration-700\"\n                    leave-from=\"translate-x-0\"\n                    leave-to=\"translate-x-full\"\n                  >\n                    <DialogPanel class=\"pointer-events-auto w-screen max-w-2xl\">\n                      <div\n                        class=\"flex h-full flex-col overflow-y-scroll bg-white py-6 shadow-xl\"\n                      >\n                        <div class=\"px-4 sm:px-6\">\n                          <div class=\"flex items-start justify-between\">\n                            <DialogTitle\n                              class=\"text-lg font-medium text-gray-900\"\n                            >\n                              Create New Application\n                            </DialogTitle>\n                            <div class=\"ml-3 flex h-7 items-center\">\n                              <button\n                                type=\"button\"\n                                class=\"rounded-md bg-white text-gray-400 hover:text-gr